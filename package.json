{
  "name": "yipyap",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:rules": "firebase emulators:exec --only firestore,storage 'NODE_OPTIONS=--experimental-vm-modules jest --config jest.rules.config.js'",
    "emulator": "firebase emulators:start --only firestore,storage",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "expo": "~54.0.14",
    "expo-constants": "~18.0.9",
    "expo-device": "~8.0.9",
    "expo-font": "^14.0.9",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-notifications": "^0.32.12",
    "expo-router": "~6.0.13",
    "expo-status-bar": "~3.0.8",
    "firebase": "^12.4.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "^0.5.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "firebase-tools": "^14.20.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.12",
    "lint-staged": "^16.2.5",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.1.0",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "private": true
}
